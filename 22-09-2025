3005. Count Elements With Maximum Frequency

#Simple using arrays
class Solution {
    public int maxFrequencyElements(int[] nums) {
        int[] count = new int[101];        
        for (int num : nums) count[num]++;
        int maxFreq = 0;
        for (int x : count) maxFreq = Math.max(maxFreq, x);
        int ans = 0;
        for (int freq : count) if (freq == maxFreq) ans += freq;
        return ans;
    }
}

