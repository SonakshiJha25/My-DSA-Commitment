#GFG
Rotate Array

class Solution {
    static void rotateArr(int arr[], int d) {
        int n = arr.length;
        d = d % n;
        int[] ans = new int[n];
        for(int i=d; i<n; i++) ans[i-d] = arr[i];
        for(int i=0; i<d; i++) ans[n-d+i] = arr[i];
        for(int i=0; i<n; i++) arr[i] = ans[i];
    }
}
